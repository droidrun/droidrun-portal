<?xml version="1.0" encoding="utf-8"?>
<!--
   Data extraction rules for API 31+.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude device_id from backup to ensure unique IDs per device/emulator -->
        <exclude domain="sharedpref" path="droidrun_device.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude device_id from device transfer to ensure unique IDs -->
        <exclude domain="sharedpref" path="droidrun_device.xml"/>
    </device-transfer>
</data-extraction-rules>